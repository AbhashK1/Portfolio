          const text =
            "Hi, I’m Abhash Kumar — a Computer Science graduate, currently working at Accenture Gurugram, where I develop scalable solutions on the Oracle ASAP platform. My passion lies in building smart, efficient systems — from full-stack web applications to AI-powered frameworks.";

          const words = text.split(' ');
          this.words_els = [];
          this.words_els = words.map((word, i) => {
            return `<span class="word">${word}</span> `;
          });

          const updateArrayWithDelay = (index = 0) => {
            if (index < this.words_els.length) {
              this.words_els[index] = this.words_els[index].replace('class="word"', 'class="word visible"');
              setTimeout(() => {
                updateArrayWithDelay(index + 1);
              }, 30);
            }
          };

          setTimeout(() => {
            updateArrayWithDelay();
          }, 60);
